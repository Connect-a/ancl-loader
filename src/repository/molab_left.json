[
  { "id": "001IUX", "text": "あたしにお任せってね！" },
  { "id": "002HVR", "text": "出発ね。" },
  { "id": "003BOS", "text": "参りましょう。" },
  { "id": "004WCO", "text": "敵であろうと抱腹絶倒間違いなしにょ！" },
  { "id": "005OGC", "text": "うう……しかたないですねぇ" },
  { "id": "006PCT", "text": "メタルなやつとか出てきませんかね" },
  { "id": "007JZK", "text": "ヤギさんのために、がんばります！" },
  { "id": "008VEF", "text": "ちょっと緊張してきたかも…！" },
  { "id": "009WHK", "text": "さぁ、怪盗シャンのおでましよ～！" },
  { "id": "010NEM", "text": "アサシンの血が騒ぐにゃ！" },
  { "id": "011RGV", "text": "さあ、大物を狙っていくよ！！" },
  { "id": "012OQV", "text": "気をつけてね" },
  { "id": "013YKO", "text": "さあいくぞ騎士殿！" },
  { "id": "014UZB", "text": "守るために、私は戦う！" },
  { "id": "015LCY", "text": "わたくしと共に行きましょう！" },
  { "id": "016ZWF", "text": "ぶちのめしてやるぜ！" },
  { "id": "017GDY", "text": "ん、漁の時間だね～！" },
  { "id": "018QDP", "text": "よしっ、気合い入れていくよ！" },
  { "id": "019IAC", "text": "私も同行しよう" },
  { "id": "020BOR", "text": "修行の成果を見せるとしよう！" },
  { "id": "021NPF", "text": "いざ往かん…闘争の地へ" },
  { "id": "022NHF", "text": "よ～し！　いっきますよ～！" },
  { "id": "023VAI", "text": "狩りを始めるよ！" },
  { "id": "024LCR", "text": "インドアなんだけどなぁ…" },
  { "id": "025YRU", "text": "べ、別に緊張なんてしてないわ！" },
  { "id": "026SWV", "text": "ひと暴れするかーっ！　いっくぞー！" },
  { "id": "027EBQ", "text": "あーしにまっかせてー！" },
  { "id": "028MQG", "text": "よーし！　張り切っていくよーっ！" },
  { "id": "029BZD", "text": "戦闘は苦手なんだがナ…。" },
  { "id": "030KKF", "text": "さぁ、行きましょう！" },
  { "id": "031VTK", "text": "余は前しか向かないもん！" },
  { "id": "032IHC", "text": "相手になったるわ！" },
  { "id": "033RJA", "text": "みんなを支えるよ！" },
  { "id": "034ICX", "text": "さあ、準備はいいかしら？　いくわよ！" },
  { "id": "035DKD", "text": "今回の獲物は…あれだな" },
  { "id": "036LLU", "text": "ふふっ、新発明を試す時…！" },
  { "id": "037FZR", "text": "がるる…怪しい匂い！" },
  { "id": "038BYN", "text": "戦闘準備だ。注意していこう。" },
  { "id": "039MZS", "text": "ボクの組んだスキーム通りにヨロ！" },
  { "id": "040GVL", "text": "ぜひご一緒させてください" },
  { "id": "042OOG", "text": "…重い腰を上げるとするか。" },
  { "id": "043AQF", "text": "ウチ、戦闘は専門外なんだけどぉ…" },
  { "id": "045LPI", "text": "一緒に頑張ろうね！" },
  { "id": "046HYB", "text": "ご一緒します！" },
  { "id": "047CJQ", "text": "私も行くとしよう" },
  { "id": "048DTB", "text": "これは演習ではない！" },
  { "id": "050LXP", "text": "あなたの剣となりましょう" },
  { "id": "051TWQ", "text": "イェーイ！ レッツパーリィ！" },
  { "id": "053QGX", "text": "行きますか。" },
  { "id": "054XLU", "text": "うん、喉の調子も良い感じ。" },
  { "id": "056AMF", "text": "準備はいい？" },
  { "id": "057QEY", "text": "一発かましてこ！" },
  { "id": "058WVJ", "text": "水辺を荒らすものは許しません！" },
  { "id": "061FZN", "text": "わたしにお任せください！" },
  { "id": "062KAS", "text": "さあ、参りましょうか！" },
  {
    "id": "063RZK",
    "text": "みんな、怪我しないように。ママについてらっしゃい！"
  },
  { "id": "064FQW", "text": "よーし！ いくわよー！" },
  { "id": "065DSR", "text": "いってらっしゃい♪" },
  { "id": "066BUY", "text": "冒険の始まりだ！" },
  { "id": "067XAC", "text": "まかせて。私、頑張っちゃうから～。" },
  { "id": "069JTH", "text": "マスターが戦えと言うのなら、私は戦います" },
  { "id": "070CDC", "text": "力を見せてやろう！" },
  { "id": "071PDW", "text": "あちき…い、イくです…！" },
  { "id": "072GZM", "text": "作戦決行よ！" },
  { "id": "073GTH", "text": "う、上手くできるかなぁ…？" },
  { "id": "074TTD", "text": "さーて、いっくよー♪" },
  { "id": "087GJX", "text": "お姉ちゃんがついてるわ" },
  { "id": "091LRY", "text": "すべて私に任せてくれ！" },
  { "id": "092VRB", "text": "王の出陣であるぞ！" },
  { "id": "093NGB", "text": "あぁん、いややぁ～。わらわのお酒～…。" },
  { "id": "094PSW", "text": "さ、行くわよ！" },
  { "id": "100VUJ", "text": "地上の平和を乱す者は許しません。" },
  { "id": "101KTR", "text": "肯定、戦闘へ向かいます。" },
  { "id": "102UPL", "text": "あたしに任せろ、です。" },
  { "id": "103UFV", "text": "頑張ります！　むん！" },
  { "id": "104OEI", "text": "いっくよー！" },
  { "id": "105RDM", "text": "わたしがついていますよ。" },
  { "id": "106TYN", "text": "じ、実戦っ！　頑張りますっ！！" },
  { "id": "107BOJ", "text": "戦いは向いてないんだけどな…。" },
  { "id": "108IXG", "text": "調停を開始する" },
  { "id": "109ICB", "text": "がんばりまーーすっ！" },
  { "id": "115MTR", "text": "さくっとやっちゃうわよ" },
  { "id": "142FWP", "text": "いっちょ派手にやるか！" },
  { "id": "150YTE", "text": "あはっ、ワクワクしてきたー！" },
  { "id": "153GZE", "text": "…めんどくさいけど、まあいいか。" },
  { "id": "160QWU", "text": "やれやれ、隠居の私が出なければならないとは" },
  { "id": "161ZEF", "text": "慌てず騒がず、参りましょう" },
  { "id": "162TRO", "text": "綺麗にしましょう。" },
  { "id": "166GBT", "text": "あ、あうぅ…戦いは苦手なの…" },
  { "id": "167YEY", "text": "さあ、いっくぞー！" },
  { "id": "168PPI", "text": "戦いは苦手なのよねぇ…" },
  { "id": "172VKK", "text": "修行の成果、見せてさしあげます。" },
  { "id": "173SDI", "text": "どけどけ～！　プルソンさまのお通りなのだー！" },
  { "id": "174AMT", "text": "ちゃちゃっとやるよ" },
  { "id": "193MSA", "text": "わらわも共にゆこう。" },
  { "id": "198CPH", "text": "よるむん、ごー！" },
  { "id": "199SDZ", "text": "さあ、狩りの時間だ。" },
  { "id": "206HSN", "text": "力をお貸ししましょう。" },
  { "id": "207VOO", "text": "労働はんたーい" },
  { "id": "208SAP", "text": "アタシが最強ってとこ見せてやるよ！" },
  { "id": "208SAP", "text": "ボクの力を見せてあげるよ！" },
  { "id": "224MPO", "text": "こ、怖いけど…がんばります…！" },
  { "id": "225XVV", "text": "拝命いたした。いざ、参る！" },
  { "id": "226MEM", "text": "手応えのある戦いだといいけど。" },
  { "id": "229YWB", "text": "余の威光を知らしめる！" },
  { "id": "235YZO", "text": "あううぅ～、死にたくないよぉ！" },
  { "id": "238YYH", "text": "よろしくおねがいしまーす♪" },
  { "id": "239ZJM", "text": "ん、仕事の時間？" },
  { "id": "241HGO", "text": "よ～し、いくよ～！" },
  { "id": "242VIG", "text": "たくさん練習してきたポーズのお披露目よ！" },
  { "id": "261BQA", "text": "やるしかないし！" },
  { "id": "263ZPQ", "text": "わ、私も戦うんですか…！？" },
  { "id": "266MBF", "text": "痛くしちゃっていいのかしら？" },
  { "id": "278TBX", "text": "邪気を感じますね。行きましょう！" },
  { "id": "284WZS", "text": "キミはもう、やられてるっす！" },
  { "id": "285YPZ", "text": "それじゃあ、始めちゃいましょうかっ！" },
  { "id": "286WBF", "text": "戦いの心得なら、多少はあります。" }
]
